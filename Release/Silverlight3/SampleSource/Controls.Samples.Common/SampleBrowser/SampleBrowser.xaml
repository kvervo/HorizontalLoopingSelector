                                                                            </Border.RenderTransform>
                                                                        </Border>
                                                                        <Path HorizontalAlignment="Center" x:Name="arrow" VerticalAlignment="Center" StrokeThickness="2" Data="M 1,1.5 L 4.5,5 L 8,1.5">
