    <SolidColorBrush x:Key="ChartBrush2" Color="{StaticResource SecondaryColor}" />

    <LinearGradientBrush x:Key="ChartBrush3" EndPoint="2,2" StartPoint="0,0" SpreadMethod="Repeat" MappingMode="Absolute">
